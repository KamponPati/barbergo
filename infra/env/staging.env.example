NODE_ENV=staging
PORT=3000
DATABASE_URL=postgresql://<staging-user>:<staging-pass>@127.0.0.1:55432/barbergo_staging
REDIS_URL=redis://127.0.0.1:56379
JWT_SECRET=<staging-secret>
AUTH_COOKIE_SECURE=true
IDEMPOTENCY_TTL_SECONDS=86400

MINIO_ENDPOINT=127.0.0.1
MINIO_PORT=59000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=<staging-minio-access-key>
MINIO_SECRET_KEY=<staging-minio-secret-key>
MINIO_BUCKET=barbergo-staging
MINIO_SIGNED_URL_EXPIRY_SECONDS=900

PAYMENT_PROVIDER_URL=<staging-payment-provider-url>
PAYMENT_PROVIDER_FALLBACK_URL=<staging-payment-provider-fallback-url>
PAYMENT_PROVIDER_API_KEY=<staging-payment-provider-api-key>
PAYMENT_PROVIDER_WEBHOOK_SECRET=<staging-payment-provider-webhook-secret>
PAYMENT_PROVIDER_TIMEOUT_MS=3000
PAYMENT_PROVIDER_MAX_RETRIES=2
PAYMENT_PROVIDER_CIRCUIT_FAILURE_THRESHOLD=3
PAYMENT_PROVIDER_CIRCUIT_COOLDOWN_MS=30000

PUSH_PROVIDER_URL=<staging-push-provider-url>
PUSH_PROVIDER_FALLBACK_URL=<staging-push-provider-fallback-url>
PUSH_PROVIDER_API_KEY=<staging-push-provider-api-key>
PUSH_PROVIDER_TIMEOUT_MS=3000
PUSH_PROVIDER_MAX_RETRIES=2
PUSH_PROVIDER_CIRCUIT_FAILURE_THRESHOLD=3
PUSH_PROVIDER_CIRCUIT_COOLDOWN_MS=30000

